{
	"name": "codestream.intellij",
	"version": "1.0.0",
	"description": "CodeStream for IntelliJ",
	"main": "index.js",
	"author": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": ""
	},
	"scripts": {
		"build": "npm run agent:build && tsx esbuild.ts --prod",
		"bundle": "npm install --no-save && npm run webview:verify && npm run agent:bundle && npm run agent:pkg && tsx esbuild.ts --prod",
		"bundle:mac": "npm run agent:bundle && npm run agent:pkg:mac && tsx esbuild.ts --prod",
		"bundle:ci": "npm run clean:ci && npm run bundle",
		"clean:ci": "git clean -xdf -e licenses/**/* && npm run webview:clean:ci && npm run agent:clean:ci",
		"rebuild": "npm install --no-save && npm run agent:rebuild && npm run build",
		"watch": "tsx esbuild.ts --watch",
		"agent:build": "pushd \"../shared/agent\" && npm run build && popd",
		"agent:bundle": "pushd \"../shared/agent\" && npm run bundle && popd",
		"agent:clean:ci": "pushd \"../shared/agent\" && npm run clean:ci && popd",
		"agent:pkg": "pushd \"../shared/agent\" && npm run pkg-jetbrains && popd",
		"agent:pkg:mac": "pushd \"../shared/agent\" && npm run pkg-jetbrains-mac && popd",
		"agent:rebuild": "pushd \"../shared/agent\" && npm run rebuild && popd",
		"agent:test": "pushd \"../shared/agent\" && npm run test && popd",
		"agent:watch": "pushd \"../shared/agent\" && npm run watch && popd",
		"webview:clean:ci": "pushd \"../shared/ui\" && npm run clean:ci && popd",
		"webview:verify": "tsx esbuild.ts --only-symlinks && pushd \"../shared/ui\" && npm run verify:compile && popd",
		"preinstall": "pushd \"../shared/ui\" && npm install --no-save && popd && pushd \"../shared/agent\" && npm install --no-save && popd && pushd \"../shared/util\" && npm install --no-save && popd"
	},
	"dependencies": {
		"core-js": "2.6.9"
	},
	"devDependencies": {
		"@babel/cli": "7.15.7",
		"@babel/core": "7.7.2",
		"@babel/plugin-proposal-class-properties": "7.7.0",
		"@babel/plugin-proposal-object-rest-spread": "7.6.2",
		"@babel/plugin-proposal-optional-chaining": "7.13.8",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.6.2",
		"@babel/preset-env": "7.7.1",
		"@babel/preset-react": "7.7.0",
		"@babel/preset-typescript": "7.7.2",
		"@babel/runtime": "7.7.2",
		"@types/keytar": "4.4.2",
		"@types/lodash-es": "4.17.6",
		"@types/node": "10.1.4",
		"@types/node-fetch": "2.6.1",
		"@types/uuid": "3.4.5",
		"@typescript-eslint/eslint-plugin": "5.13.0",
		"@typescript-eslint/eslint-plugin-tslint": "5.13.0",
		"@typescript-eslint/parser": "5.13.0",
		"babel-loader": "8.2.3",
		"babel-plugin-styled-components": "2.0.2",
		"circular-dependency-plugin": "5.2.2",
		"css-loader": "6.5.1",
		"esbuild": "0.14.50",
		"esbuild-plugin-alias": "0.2.1",
		"esbuild-plugin-less": "1.1.8",
		"eslint": "6.8.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-import": "2.20.0",
		"less": "4.1.2",
		"mini-css-extract-plugin": "2.4.5",
		"prettier": "1.19.1",
		"tslint": "6.1.3",
		"tsx": "^3.8.0",
		"typescript": "4.5.4"
	}
}
